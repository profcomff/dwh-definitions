from datetime import datetime

import sqlalchemy as sa
from sqlalchemy.orm import Mapped, mapped_column

from profcomff_definitions.base import Base


class ods_timetable_act(Base):
    """"""
    id: Mapped[int] = mapped_column(nullable = False)
    event_text: Mapped[str] = mapped_column(comment="Мероприятие", nullable = False)
    time_interval: Mapped[str] = mapped_column(comment="Время",nullable = False)
    group_text: Mapped[str] = mapped_column(comment="Номер группы", nullable = False)
